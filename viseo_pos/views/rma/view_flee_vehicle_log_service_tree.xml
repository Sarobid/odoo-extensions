<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <odoo>
    <record id="viseo_pos_flee_vehicle_log_service_tree_id" model="ir.ui.view">
        <field name="name">viseo.pos.flee.vehicle.log.service.tree.id</field>
        <field name="model">fleet.vehicle.log.services</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_log_services_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <field name="nbr_service_product_block" invisible="1"/>
            </xpath>
            <xpath expr="//tree" position="attributes">
                <attribute name="decoration-danger">nbr_service_product_block > 0</attribute>
            </xpath>
    
        </field>
    </record>
</odoo>

</odoo>
